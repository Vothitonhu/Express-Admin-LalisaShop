<table class="table table-hover">
    <thead>
        <tr>
            <th>Product name</th>
            <th>Price</th>
            <th>Description</th>
        </tr>
    </thead>
    <% products.forEach((entry) => { %>
    <tr>
        <td><%= entry.name %></td>
        <td><%= entry.price %></td>
        <td><%= entry.description %></td>
    </tr>
    <% }) %>
</table>
